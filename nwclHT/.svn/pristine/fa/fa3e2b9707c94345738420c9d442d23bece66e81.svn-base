  <select id="getUserAllCoupon" parameterType="map" resultMap="BizUserCouponModelMap">
  select a.id,b.img_path,b.`start`,b.`end`,c.title,b.description,b.time,a.use_time,a.serial,(case when(b.`end` &gt; CURRENT_TIMESTAMP()) then '0' else '1' end) isEnd from n_buser_coupon a
	left outer join n_coupon b on a.coupon_id=b.id
