    select a.biz_user_id id,a.room_id,a.type,a.status,b.nickname,b.head_portrait,b.phone,b.row_add_time,CONCAT(e.`name`,d.`name`,c.floor,c.room_no) address from n_biz_user_room a
	left outer join n_biz_user b on a.biz_user_id = b.id
