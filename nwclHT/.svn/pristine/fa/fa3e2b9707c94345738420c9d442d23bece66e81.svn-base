  <select id="getUserRooms" parameterType="java.lang.String" resultMap="RoomModelMap">
  	<!-- 获取系统预留的业主房间 -->
    select r.id id,b.name building_name,c.name community_name,floor,unit,room_no roomNo from n_room r
	left outer join n_building b ON r.building_id = b.id
	left outer join n_community c ON c.id = b.community_id
	where r.id in (select room_id from n_room_phone where phone = #{id,jdbcType=VARCHAR} )
  </select>
  
  <select id="getUserActiveRooms" parameterType="java.lang.Long" resultMap="RoomModelMap">
  	<!-- 获取业主已经绑定,并且自己是业主的房间 -->
    select r.id id,b.name building_name,c.name community_name,floor,unit,room_no roomNo from n_room r
	left outer join n_building b ON r.building_id = b.id
	left outer join n_community c ON c.id = b.community_id
	where r.id in (select room_id from n_biz_user_room where biz_user_id = #{id,jdbcType=BIGINT} and type = '业主' )
  </select>
  
  <select id="getUserAllRooms" parameterType="java.lang.Long" resultMap="RoomModelMap">
  	<!-- 获取用户的所有房间 -->
	select (case when z.current_room_id = a.room_id then 1 else 0 end ) isCurrent,r.id id,b.name building_name,c.name community_name,floor,unit,room_no roomNo,a.type, case when a.type != '业主' then (select `name` from n_biz_user where id = a.authorize_user_id) else '' end authorizeUserName, case when a.type != '业主' then (select `nickname` from n_biz_user where id = a.authorize_user_id) else '' end authorizeUserNickname, case when a.type != '业主' then (select `phone` from n_biz_user where id = a.authorize_user_id) else '' end authorizeUserPhone from n_biz_user_room a left outer join n_room r on a.room_id = r.id left outer join n_building b ON r.building_id = b.id left outer join n_community c ON c.id = b.community_id left outer join n_biz_user z on a.biz_user_id = z.id where a.biz_user_id = #{id,jdbcType=BIGINT}
  </select>
  
