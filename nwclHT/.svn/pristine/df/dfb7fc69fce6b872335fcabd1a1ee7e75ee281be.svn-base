package com.bola.nwcl.api.model;

import java.util.Date;

import com.wordnik.swagger.annotations.ApiModelProperty;

public class BizUserCouponModel {
	private long id;
	private Date start;
	private Date end;
	private String title;
	private String description;
	private int time;
	private int useTime;
	private int isEnd;
	private String serial;
	private String imgPath;
	
	@ApiModelProperty(value = "优惠卷描述")
	public String getDescription() {
		return description;
	}
	@ApiModelProperty(value = "结束时间")
	public Date getEnd() {
		return end;
	}
	@ApiModelProperty(value = "id")
	public long getId() {
		return id;
	}
	@ApiModelProperty(value = "优惠卷图片")
	public String getImgPath() {
		return imgPath;
	}
	@ApiModelProperty(value = "是否过期,1:已经过期,0:没过期")
	public int getIsEnd() {
		return isEnd;
	}
	@ApiModelProperty(value = "优惠卷编号")
	public String getSerial() {
		return serial;
	}
	@ApiModelProperty(value = "开始时间")
	public Date getStart() {
		return start;
	}
	@ApiModelProperty(value = "一共可以使用的次数,-1表示无限次")
	public int getTime() {
		return time;
	}
	@ApiModelProperty(value = "商店标题")
	public String getTitle() {
		return title;
	}
	@ApiModelProperty(value = "已经使用的次数")
	public int getUseTime() {
		return useTime;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public void setEnd(Date end) {
		this.end = end;
	}
	public void setId(long id) {
		this.id = id;
	}
	public void setImgPath(String imgPath) {
		this.imgPath = imgPath;
	}
	public void setIsEnd(int isEnd) {
		this.isEnd = isEnd;
	}
	public void setSerial(String serial) {
		this.serial = serial;
	}
	public void setStart(Date start) {
		this.start = start;
	}
	public void setTime(int time) {
		this.time = time;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public void setUseTime(int useTime) {
		this.useTime = useTime;
	}
	
}
