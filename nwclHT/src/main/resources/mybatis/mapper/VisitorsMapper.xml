  <select id="getSercurityVisitorsModel" parameterType="map" resultMap="SercurityVisitorsModelMap">
  	select (select count(*) from n_visitors_statistics_detail z where z.visitors_id = a.id) visitorsCount,a.id,a.serial,a.effective_arrive_time,a.effective_leave_time,a.expect_arrive_time,a.expect_leave_time,b.phone,b.`name`,b.nickname,c.type identity,CONCAT(f.`name`,e.`name`,d.floor,d.room_no) roomStr,h.id keeperid,h.`name` keepername,i.serial keeperno from n_visitors a left outer join n_biz_user b on a.buser_id=b.id left outer join n_biz_user_room c on b.id=c.biz_user_id and a.room_id = c.room_id left outer join n_room d on a.room_id= d.id 
	left outer join n_building e on e.id=d.building_id left outer join n_community f on e.community_id=f.id
	left outer join n_housekeeper_scheduling g on g.building_id=e.id and DATE_FORMAT(g.duty_time,'%Y-%m-%d') = CURDATE() left outer join n_service_user h on h.id=g.housekeeper_id
	left outer join n_housekeeper_detail i on i.keeper_id=h.id
	where 1=1
	<if test="id != null">
